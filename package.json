{
  "name": "@magmooty/api",
  "version": "1.2.0",
  "description": "Magmooty Monolith",
  "main": "dist/index.js",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@magmooty"
  },
  "scripts": {
    "start": "TS_NODE_BASEURL=./dist node -r tsconfig-paths/register ./dist/src/index.js",
    "build": "npx rimraf dist && npx typescript-transpile-only && cp -r ./src/errors/i18n ./dist/src/errors/i18n && cp -r ./src/value-sets/i18n ./dist/src/value-sets/i18n && cp -r ./src/value-sets/sets ./dist/src/value-sets/sets",
    "gen:types": "ts-node -r tsconfig-paths/register src/scripts/generate-types.ts",
    "start:dev": "nodemon",
    "test:prepare": "./prepare-test-env.sh",
    "test": "./wait-before-tests.sh && NODE_ENV=test jest --ci --reporters=default --reporters=jest-junit --runInBand dist",
    "clean": "rm -rf dist",
    "lint": "eslint . --fix",
    "seed": "ts-node -r tsconfig-paths/register src/seeds/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/magmooty/api.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/magmooty/api/issues"
  },
  "homepage": "https://github.com/magmooty/api#readme",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.1.1",
    "@types/joi-phone-number": "^5.0.4",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.184",
    "@types/milliseconds": "^0.0.30",
    "@types/node": "^18.0.1",
    "@types/string-template": "^1.0.2",
    "@types/uuid": "^8.3.4",
    "@types/yargs": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.19.0",
    "ts-node": "^10.8.2",
    "jest-junit": "^14.0.1"
  },
  "dependencies": {
    "@faker-js/faker": "^7.5.0",
    "@aws-sdk/client-dynamodb": "^3.142.0",
    "@elastic/elasticsearch": "^8.2.1",
    "@escapace/accept-language-parser": "^1.4.4",
    "@slack/web-api": "^6.7.2",
    "@stdlib/error-to-json": "^0.0.7",
    "@tsconfig/node16": "^1.0.3",
    "aws-sdk": "^2.1189.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-user-agent-blocker": "^2.0.4",
    "ioredis": "^5.2.2",
    "jest": "^29.1.2",
    "jest-serial-runner": "^1.2.0",
    "joi": "^17.6.0",
    "joi-phone-number": "^5.1.1",
    "json-schema": "^0.4.0",
    "jsonwebtoken": "^8.5.1",
    "junit2json": "^3.0.3",
    "kafkajs": "^2.1.0",
    "kuuid": "^1.0.1",
    "lodash": "^4.17.21",
    "milliseconds": "^1.0.3",
    "moment": "^2.29.4",
    "mongodb": "^4.10.0",
    "nanoid": "^3.3.4",
    "prettier": "^2.7.1",
    "prom-client": "^14.0.1",
    "prometheus-api-metrics": "^3.2.2",
    "queue": "^6.0.2",
    "runtypes": "^6.6.0",
    "slack-block-builder": "^2.6.0",
    "string-template": "^1.0.0",
    "superagent": "^8.0.2",
    "tiny-timestamp": "^1.0.0",
    "tokenizr": "^1.6.7",
    "ts-jest": "^29.0.3",
    "ts-json-schema-generator": "^1.0.0",
    "tsconfig-paths": "^4.0.0",
    "tsconfig-paths-jest": "^0.0.1",
    "twilio": "^3.80.1",
    "typescript-transpile-only": "^0.0.4",
    "yargs": "^17.5.1"
  }
}
